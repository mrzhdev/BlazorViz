@inject IJSRuntime JSRuntime
@inject NavigationManager NavManager
@using Microsoft.JSInterop
@using DummyClassLibrary
@page "/"


<PageTitle>Graphviz visualisation for C#</PageTitle>
<VizGraph></VizGraph>


